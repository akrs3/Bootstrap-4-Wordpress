
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /bootstrap-4-wordpress/bootstrap-4-wordpress/tema/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /bootstrap-4-wordpress/bootstrap-4-wordpress/tema/index.php [L]
</IfModule>

# END WordPress